@article{morer_comparing_2018,
	title = {Comparing spatial networks: {A} 'one size fits all' efficiency-driven approach},
	shorttitle = {Comparing spatial networks},
	url = {http://arxiv.org/abs/1807.00565},
	abstract = {Spatial networks are a very powerful framework for studying a large variety of systems which can be found in a broad diversity of contexts: from transportation, to biology, from epidemiology, to communications, and migrations, to cite a few. In spatial networks, distances between node pairs can be regarded as natural weights associated to links, generally understood as the amount of resources needed for building or traveling such connections. Consequently, a network as a whole is characterized by a {\textbackslash}emph\{total cost\} given by the sum of the costs of the links in them. Here, we address the issue of how to gauge the quality of spatial network designs by proposing a two-step methodology. Firstly, we introduce a quality function to assess the overall performance of a network. Then, we devise an algorithm for building connectivity patterns which maximizes the value of such a function, while satisfying the same constraints ({\textbackslash}ie, node positions and total cost) of their empirical counterparts. Hence, the smaller is the difference between the maximal and the empirical value of the quality function, the higher the quality of the design of the network.},
	urldate = {2018-07-04},
	journal = {arXiv:1807.00565 [physics]},
	author = {Morer, Ignacio and Cardillo, Alessio and Diaz-Guilera, Albert and Prignano, Luce and Lozano, Sergi},
	month = jul,
	year = {2018},
	note = {arXiv: 1807.00565},
	keywords = {Physics - Physics and Society, Computer Science - Social and Information Networks},
	annote = {Comment: 9 pages, 5 figures},
	file = {arXiv\:1807.00565 PDF:/Users/jmateosgarcia/Zotero/storage/D9C6C8RK/Morer et al. - 2018 - Comparing spatial networks A 'one size fits all' .pdf:application/pdf;arXiv.org Snapshot:/Users/jmateosgarcia/Zotero/storage/7XSU8T9C/1807.html:text/html}
}

@article{balister_topological_2018,
	title = {Topological {Phase} {Transitions} in {Spatial} {Networks}},
	url = {http://arxiv.org/abs/1806.10114},
	abstract = {Most social, technological and biological networks are embedded in a finite dimensional space, and the distance between two nodes influences the likelihood that they link to each other. Indeed, in social systems, the chance that two individuals know each other drops rapidly with the distance between them; in the cell, proteins predominantly interact with proteins in the same cellular compartment; in the brain, neurons mainly link to nearby neurons. Most modeling frameworks that aim to capture the empirically observed degree distributions tend to ignore these spatial constraints. In contrast, models that account for the role of the physical distance often predict bounded degree distributions, in disagreement with the empirical data. Here we address a long-standing gap in the spatial network literature by deriving several key network characteristics of spatial networks, from the analytical form of the degree distribution to path lengths and local clustering. The mathematically exact results predict the existence of two distinct phases, each governed by a different dynamical equation, with distinct testable predictions. We use empirical data to offer direct evidence for the practical relevance of each of these phases in real networks, helping better characterize the properties of spatial networks.},
	urldate = {2018-07-04},
	journal = {arXiv:1806.10114 [cond-mat, physics:physics]},
	author = {Balister, Paul and Song, Chaoming and Riordan, Oliver and Bollobas, Bela and Barabasi, Albert-Laszlo},
	month = jun,
	year = {2018},
	note = {arXiv: 1806.10114},
	keywords = {Physics - Physics and Society, Condensed Matter - Statistical Mechanics},
	annote = {Comment: 15 pages, 3 figures},
	file = {arXiv\:1806.10114 PDF:/Users/jmateosgarcia/Zotero/storage/LBJ2YL5L/Balister et al. - 2018 - Topological Phase Transitions in Spatial Networks.pdf:application/pdf;arXiv.org Snapshot:/Users/jmateosgarcia/Zotero/storage/AXKYCJS9/1806.html:text/html}
}

