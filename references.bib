	keywords = {Physics - Physics and Society},
	file = {arXiv\:1807.03374 PDF:/Users/jmateosgarcia/Zotero/storage/JX84Y3MS/O'Clery et al. - 2018 - Productive Ecosystems and the Arrow of Development.pdf:application/pdf;arXiv.org Snapshot:/Users/jmateosgarcia/Zotero/storage/5X6QC5LB/1807.html:text/html}
}

